<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.mixed.exam.teacher.dao.UsersMapper">
    <select id="getUsersDetail" resultType="org.mixed.exam.auth.api.po.Users">
        select *
        from users
        where username = #{username}
    </select>
</mapper>