<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.mixed.exam.admin.mapper.RoleMapper">
    <update id="updateMul">


    </update>

    <select id="findAll"
            resultType="org.mixed.exam.admin.pojo.dto.usersDto">

        SELECT
            users.username,users.realName,users.role
        from users
        WHERE username=#{0}
        order by time desc
    </select>
</mapper>