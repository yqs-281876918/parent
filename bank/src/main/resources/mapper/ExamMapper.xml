<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.mixed.exam.bank.dao.ExamMapper">
    <insert id="addExam">
        insert into exam(paperID,testTime,startTime,lateTime,submitTime,classID,antiSettings)
        values(#{paperID},#{testTime},#{startTime},#{lateTime},#{submitTime},#{classID},#{antiSettings})
    </insert>
</mapper>